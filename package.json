{
  "name": "nasonero-11ty-vite",
  "version": "0.1",
  "description": "Our personal digital playground",
  "browserslist": [
    "> 0.5%",
    "not IE 11"
  ],
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "dev": "npx @11ty/eleventy --serve",
    "watch": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "build": "env NODE_ENV=production npx @11ty/eleventy --pathprefix 'nasonero'",
    "postbuild": "env NODE_ENV=production npx pagefind --source _site --glob '**/*.html'",
    "clean": "del-cli _site",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Fupete@github.com/Fupete/nasonero.git"
  },
  "author": "Fupete",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/Fupete/nasonero/issues"
  },
  "homepage": "https://github.com/Fupete/nasonero/",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^6.0.0",
    "cssremedy": "^0.1.0-beta.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "focus-visible": "^5.2.0",
    "luxon": "^3.0.1",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "npm-run-all": "^4.1.5",
    "pagefind": "^0.12.0",
    "postcss-nesting": "^11.2.2",
    "rollup-plugin-critical": "^1.0.12",
    "sass": "^1.53.0",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "photoswipe": "^5.3.7"
  }
}
