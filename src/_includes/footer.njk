<footer class="site-footer" role="contentinfo">
    <div class="site-footer__inner container">

			<div id="langAlternatives">
				{% for link in page.url | locale_links %}
					{%- if loop.first %}
					This page is also available in:
					{% endif %}
					{%- if not loop.first %},{% endif %}
					<a href="{{link.url | url}}" lang="{{link.lang}}" hreflang="{{link.lang}}">{{link.label}}</a>
				{% endfor %}
			</div>

        <p><a href="https://github.com/fupete/nasonero/blob/main/{{ page.inputPath }}">Edit this page on GitHub</a><p>
        <p><span>&copy; 2000-{{ build.timestamp | dateToFormat('yyyy') }} - Erika Gabbani & Daniele Tabellini</span></p>
    </div>
</footer>
