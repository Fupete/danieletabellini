---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - idea
    - ideas
    - tagList
  addAllPagesToCollections: true
layout: base
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /en/tags/{{ tag | slugify }}/
date: git Last Modified
---

<div class="container">
  <h1>Ideas tagged "{{ tag }}"</h1>
  {% set tagUrl %}/en/tags/{% endset %}
  <p><a href="{{tagUrl | url}}">Browse all tags</a></p>
  {% set ideaslist = collections[tag] %}
  {% include "ideaslist.njk" %}
</div>





